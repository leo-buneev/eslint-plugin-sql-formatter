{
  "name": "eslint-plugin-sql-formatter",
  "version": "1.0.5",
  "description": "ESLint plugin for formatting *.sql files and sql`...` template strings inside *.js files",
  "main": "src/index.js",
  "repository": "https://github.com/leo-buneev/eslint-plugin-sql-formatter",
  "author": "Leonid Buneev <leo.buneev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel ./src --out-dir ./dist --copy-files",
    "lint": "eslint ./src ./test",
    "precommit": "npm run lint && npm run test",
    "test": "mocha --recursive --reporter progress --timeout 9000"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "eslint-plugin-tyrecheck": "^2.45.1",
    "fs-extra": "^10.1.0"
  },
  "dependencies": {
    "astring": "^1.8.3",
    "mocha": "^10.0.0",
    "sql-formatter": "^8.2.0",
    "typescript": "^4.7.4"
  },
  "keywords": [
    "eslint",
    "plugin",
    "sql",
    "prettier",
    "formatter"
  ]
}
